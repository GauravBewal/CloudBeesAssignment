# Test source

```ts
   1 | import { test, expect, Page } from '@playwright/test';
   2 | import { allure } from 'allure-playwright';
   3 |
   4 | test.describe('CloudBees Website Tests', () => {
   5 |   let page: Page;
   6 |
   7 |   test.beforeEach(async ({ browser }) => {
   8 |     const context = await browser.newContext();
   9 |     page = await context.newPage();
  10 |     await page.goto('https://www.cloudbees.com/');
  11 |     await page.waitForLoadState('domcontentloaded');
  12 |   });
  13 |
  14 |   test('Verify CloudBees CD/RO and Documentation', async () => {
  15 |     allure.feature('CloudBees Website');
  16 |     allure.story('CD/RO and Documentation Verification');
  17 |
  18 |     await allure.step('Click Products > CloudBees CD/RO', async () => {
  19 |       await page.locator("//button[contains(text(),'Products')]").click();
  20 |       await page.locator("//a[@data-test='navbar.menuLink.products.otherProducts.cloudbeesCD']").click();
  21 |       await page.locator("//p[contains(text(),'Cost Savings')]/parent::div//span").waitFor({state: "visible", timeout: 5000});
  22 |     });
  23 |
  24 |     await allure.step('Verify Cost Savings value is $2m', async () => {
  25 |       const costSavingsText = await page.locator("//p[contains(text(),'Cost Savings')]/parent::div//span").textContent();
  26 |       expect(costSavingsText).toContain('$2m');
  27 |     });
  28 |
  29 |     await allure.step('Scroll down and click Auditors / Security', async () => {
  30 |       await Promise.all([
  31 |         await page.locator("//span[contains(text(),'Auditors / Security')]/ancestor::button").scrollIntoViewIfNeeded(),
  32 |         await page.locator("//span[contains(text(),'Auditors / Security')]/ancestor::button").click(),
  33 |         await page.waitForTimeout(5000)
  34 |       ]);
  35 |     });
  36 |
  37 |     await allure.step('Verify text under Release Governance', async () => {
  38 |       const releaseGovernanceText = await page.locator("//p[contains(text(),'Release Governance')]/following-sibling::h4").textContent();
  39 |       expect(releaseGovernanceText).toContain('Generate single-click audit reports');
  40 |     });
  41 |
  42 |     await allure.step('Click Resources > Documentation and verify new tab opens', async () => {
  43 |       const [newPage] = await Promise.all([
  44 |         page.waitForEvent('popup'),
  45 |         page.locator("//button[contains(text(),'Resources')]").click(),
  46 |         page.locator("//a[@data-test='navbar.menuLink.resources.supportDocumentation.documentation']").click(),
  47 |         page.waitForTimeout(5000)
  48 |       ]);
  49 |       expect(newPage.url()).not.toBe(page.url());
  50 |     await newPage.bringToFront();
  51 |     page = newPage; // Bring the new tab to focus
  52 |     });
  53 |
  54 |     await allure.step('Click in the Search field and verify new page opens', async () => {
  55 |       await page.locator("//input[@placeholder='Search all CloudBees Resources']").waitFor({state: "visible", timeout: 10000});
  56 |       await page.locator("//input[@placeholder='Search all CloudBees Resources']").click();
> 57 |       await page.locator("//input[@placeholder='Search']").waitFor({state: "visible", timeout: 10000});
     |                                                            ^ TimeoutError: locator.waitFor: Timeout 10000ms exceeded.
  58 |     });
  59 |
  60 |     await allure.step('Search for "Installation" and verify pagination', async () => {
  61 |       await page.locator("//input[@placeholder='Search']").fill('Installation');
  62 |       await page.locator("//ul[contains(@class,'pagination')]").waitFor({state: "visible", timeout: 10000});
  63 |       const pagination = page.locator("//ul[contains(@class,'pagination')]");
  64 |       expect(await pagination.isVisible()).toBe(true);
  65 |     });
  66 |   });
  67 | });
```